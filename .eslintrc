{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "rules": {
    "babel/generator-star-spacing": 1,
    "babel/new-cap": 1,
    "babel/object-curly-spacing": 1,
    "babel/object-shorthand": 1,
    "babel/arrow-parens": 1
  },
  "ecmaFeatures": {
      "blockBindings": true,
      "forOf": true,
      "jsx": true
  },
  "env": {
    "node": true,
    "browser": true
  },

  "globals": {
    "L": true
  },
  "plugins": [
    "babel"
  ]
}
