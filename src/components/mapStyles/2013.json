{
  "id": "2013",
  "type": "line",
  "source": "logging",
  'source-layer': 'original',
  "minzoom": 6,
  "maxzoom": 12,
  "filter": [
    "any",
    ["==","start_date","2013"],
    ["==","start_date","before 2013"]
  ],
  "paint": {
    "line-color": "#e46c1c",
    "line-opacity": 1,
    "line-cap": "butt",
    "line-join": "miter",
    "line-width": {
      "base": 1,
      "stops": [[7, 1.5], [8, 2], [9, 3], [10, 3.5], [11, 4], [12, 6]]
    }


  }
}
